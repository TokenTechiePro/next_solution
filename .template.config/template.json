{
  "$schema": "http://json.schemastore.org/template",
  "author": "@prince272",
  "classifications": [ "Web", "ASP.NET" ],
  "identity": "NextSolution.Template",
  "name": "Next Solution (ASP.NET API)",
  "description": "A starter solution template for rapidly creating a modern, efficient codebase for your upcoming application.",
  "shortName": "nextsoln",
  "sourceName": "NextSolution",
  "preferNameDirectory": true,
  "tags": {
    "type": "solution",
    "language": "C#"
  },
  "sources": [
    {
      "exclude": [

        "**/*.filelist",
        "**/*.lock.json",

        "**/*.rsuser",
        "**/*.suo",
        "**/*.user",
        "**/*.userosscache",
        "**/*.sln.docstates",

        "**/[Dd]ebug/**",
        "**/[Dd]ebugPublic/**",
        "**/[Rr]elease/**",
        "**/[Rr]eleases/**",
        "**/x64/**",
        "**/x86/**",
        "**/[Ww][Ii][Nn]32/**",
        "**/[Aa][Rr][Mm]/**",
        "**/[Aa][Rr][Mm]64/**",
        "**/bld/**",
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        "**/[Oo]ut/**",
        "**/[Ll]og/**",
        "**/[Ll]ogs/**",

        ".template.config/**/*",

        "**/.vs/**",

        "**/.git/**",
        "**/*.gitattributes",
        "**/*.gitignore",

        "**/*.nuspec"
      ]
    }
  ],
  "symbols": {
    "DatabaseIdGenerated": {
      "type": "generated",
      "generator": "guid",
      "replaces": "{{DatabaseIdGenerated}}",
      "parameters": {
        "format": "N"
      }
    },
    "HttpPortGenerated": {
      "type": "generated",
      "generator": "port",
      "parameters": {
        "low": 5000,
        "high": 5400
      },
       "replaces": "5282"
    },
    "HttpsPortGenerated": {
      "type": "generated",
      "generator": "port",
      "parameters": {
        "low": 7000,
        "high": 7400
      },
       "replaces": "7135"
    }
  }
}